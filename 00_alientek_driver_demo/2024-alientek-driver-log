arm-linux-gnueabihf-gcc atomcApp.c -o atomcApp
scp ./beep.ko root@192.168.10.101:/home/zaichen/07/
scp ./beepApp root@192.168.10.101:/home/zaichen/07/
insmod atomic.ko

./atomicApp /dev/gpioled  0 
./atomicApp /dev/gpioled  1 